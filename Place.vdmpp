class Place
    instance variables
        public id: nat;
        public label: set of char;
        public capacity: nat;
    
    operations
        public Place: nat * set of char * nat ==> Place
        Place (newId, newLabel, newCapacity) == 
            (id := newId; label := newLabel; capacity := newCapacity; return self)
        pre newCapacity > 0

end Place

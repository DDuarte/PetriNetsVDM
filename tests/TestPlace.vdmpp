class TestPlace is subclass of MyTestCase

operations
  public testConstructor: () ==> ()
  testConstructor() == (
    dcl p : Place := new Place("test1", 1);
    assertEqual("test1",  p.label);
    assertEqual(1,  p.capacity);
  );

  -- Entry point that runs all tests with valid inputs
  public testAll: () ==> ()
  testAll() == (
    testConstructor();
  );

end TestPlace
